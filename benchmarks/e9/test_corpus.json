{
  "version": "1.0",
  "description": "E9 HDC benchmark corpus for blind-spot detection testing",
  "metadata": {
    "created": "2026-01-25",
    "purpose": "Measure E9 contribution to search quality via typo tolerance and cross-tokenization discovery"
  },
  "categories": {
    "typo": {
      "description": "Queries with common typos - E9 should find correct memories via character trigrams",
      "expected_e9_contribution": ">=20%"
    },
    "code_id": {
      "description": "Code identifiers with different naming conventions - E9 should bridge camelCase/snake_case",
      "expected_e9_contribution": ">=15%"
    },
    "baseline": {
      "description": "Clean queries - E9 should NOT add false discoveries",
      "expected_false_positive_rate": "<=5%"
    }
  },
